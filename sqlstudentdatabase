CREATE TABLE student(
    student_id INT
    name VARCHAR(150),
    major VARCHAR(100),
    PRIMARY KEY(student_id);
);
DESCRIBE student;
ALTER TABLE student ADD gps DECIMAL(3,2);
ALTER TABLE student DROP COLUMN gpa;
);

